}

.ss-dropdown {
  height: 300px;
  width: 600px;
  background-color: white;
  position: absolute;
  right: 0;
  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
  display: flex;
  flex-direction: column;
  margin: 17vh auto;
  left: 0;
  box-sizing: border-box;
  z-index: 3;
  top: 0;
}

.ss-dropdown-msg {
  font-size: 18px;
}

.ss-dropdown-msg::before {
  content: "✅ ";
}

.ss-dropdown-nav {
  display: flex;
  padding: 15px;
  background-color: darkseagreen;
  justify-content: space-between;
  align-items: baseline;
}

.ss-dropdown-close {
  font-size: 25px;
  line-height: 20px;
  font-family: sans-serif;
  font-weight: 100;
  cursor: pointer;
}

.ss-dropdown form {
  height: 100%;
  display: flex;
  flex-direction: column;
  padding: 20px;
  justify-content: space-around;
}

.ss-dropdown form label {
  width: 100%;
}

.ss-dropdown form input {
  width: 100%;
  margin: 10px 0;
  height: 40px;
  font-size: 16px;
  padding: 0 15px;
  border-radius: 4px;
  border: 1px solid lightgrey;
  background-color: #d3d3d33d;
  box-sizing: border-box;
}

.ss-dropdown form button {
  width: 100%;
  line-height: 30px;
  display: block;
  padding: 5px;
  font-size: 16px;
  border-radius: 4px;
  border: none;
  background-color: rgba(119, 136, 153, 0.801);
  transition-duration: 0.2s;
  cursor: pointer;
  color: white;
}

.ss-dropdown form button:hover {
  background-color: rgb(119, 136, 153);
}

.ss-dropdown-screen {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 2;
  background-color: rgba(0, 0, 0, 0.192);
}

.form-errors {
  color: rgb(163, 0, 11);
}

.line-form-errors {
  line-height: 14px;
  font-size: 14px;
  margin: -10px 0 10px 0;
  color: rgb(163, 0, 11);
}

.error {
  border-color: rgb(163, 0, 11);
}

