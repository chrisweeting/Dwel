  /* border: 1px solid lightskyblue; */
  /* outline: none; */
  /* box-shadow: 0 4px 8px 0 lightskyblue, 0 6px 10px 0 lightskyblue; */
}

.search-container {
  position: relative;
  height: 50px;
}

.sticky {
  position:fixed;
  top:0;
  z-index:100;
  margin: 0 auto;
}

.stick {
  box-shadow: none;
  border: 1px solid lightslategray;
  height: 50px;
}

.active {
  position: fixed;
  top: 0;
